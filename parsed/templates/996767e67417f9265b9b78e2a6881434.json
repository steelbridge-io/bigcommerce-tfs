{
  "pages/order-confirmation": "---\nblog:\n    recent_posts:\n        limit: {{theme_settings.footer_blog_posts_count}}\n---\n\n{{#partial \"head\"}}\n\n{{{stylesheet '/assets/css/theme.css'}}}\n{{{ checkout.checkout_head }}}\n{{{stylesheet '/assets/css/parts-warehouse.css'}}}\n{{{ stylesheet '/assets/css/optimized-checkout.css' }}}\n{{ getFontsCollection }}\n\n<script type=\"text/javascript\">\n    window.language = {{{langJson 'optimized_checkout'}}};\n</script>\n\n{{{head.scripts}}}\n\n{{/partial}}\n\n{{#partial \"page\"}}\n\n<svg data-src=\"{{cdn 'img/icon-sprite.svg'}}\" class=\"icons-svg-sprite\"></svg>\n\n{{> components/custom/checkout-header}}\n\n{{{ checkout.order_confirmation_content }}}\n\n{{> components/common/checkout-footer}}\n\n{{{ footer.scripts }}}\n\n{{/partial}}\n\n{{> layout/checkout}}\n",
  "components/custom/checkout-header": "<svg data-src=\"{{cdn 'img/icon-sprite.svg'}}\" class=\"icons-svg-sprite\"></svg>\n<div class=\"top-menu\">\n    <div class=\"content\">\n         <a href=\"#\" class=\"mobileMenu-toggle\" data-mobile-menu-toggle=\"menu\">\n            <span class=\"mobileMenu-toggleIcon\">Toggle menu</span>\n        </a>\n\n        <!-- OPTIONAL SHORT MESSAGE LINK -->\n        <!-- <div class=\"left\">\n          <a class=\"optional-message-link\" href=\"\">Put an optional message here.</a>\n        </div> -->\n\n        <div class=\"left phone-number\">\n            {{#if settings.phone_number}}\n                {{settings.phone_number}}\n            {{/if}}\n        </div>\n\n        <div class=\"right account-links\">\n            <ul>\n               {{#if settings.gift_certificates_enabled}}\n                    <li class=\"navUser-item gift-certficates\">\n                        <a class=\"navUser-action\" href=\"{{urls.gift_certificate.purchase}}\">{{lang 'cart.gift_certificate'}}</a>\n                    </li>\n                {{/if}}\n                {{#if settings.account_creation_enabled}}\n                    <li class=\"navUser-item navUser-item--account\">\n                        {{#if customer}}\n                            <a class=\"navUser-action account\" href=\"{{urls.account.index}}\"><i class=\"fa fa-user\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-user-alt\" /></svg></i></a>\n                            <a class=\"navUser-action logout\" href=\"{{urls.auth.logout}}\">{{lang 'common.logout'}}</a>\n                        {{else}}\n                            <a class=\"navUser-action\" href=\"{{urls.auth.login}}\"><!-- <i class=\"fa fa-user\" aria-hidden=\"true\"></i> --><a class=\"navUser-action login\" href=\"{{urls.auth.login}}\">{{lang 'common.login'}}</a><span class=\"or-text\">or</span><a class=\"navUser-action create\" href=\"{{urls.auth.create_account}}\">{{lang 'Sign Up'}}</a>\n                        {{/if}}\n                     </li>\n                {{/if}}\n        <li class=\"navUser-item navUser-item--cart\">\n            <a\n                class=\"navUser-action\"\n                data-cart-preview\n                data-dropdown=\"cart-preview-dropdown\"\n                data-options=\"align:right\"\n                href=\"{{urls.cart}}\">\n                <span class=\"navUser-item-cartLabel\"><i class=\"cart-icon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-cart\" /></svg></i></span> <span class=\"countPill{{#if cart.items}} countPill--positive{{/if}} cart-quantity\">{{cart.quantity}}</span>\n            </a>\n\n            <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\n        </li>\n            </ul>\n        </div>\n        <div class=\"clear\"></div>\n    </div>\n</div>\n\n<div class=\"header-container full-width {{#if theme_settings.slider_width '===' 'full-width'}} full-width-auto {{/if}} full-width{{#if theme_settings.header-layout '!==' 'full-width'}} in-container{{/if}}\">\n  <div class=\"header-padding{{#if theme_settings.header-layout '===' 'in-container-with-padding-no-margin'}} in-container-with-padding-no-margin{{/if}}\">\n  <div class=\"header-padding{{#if theme_settings.header-layout '===' 'in-container-with-padding'}} in-container-with-padding{{/if}}\">\n  <header class=\"header\" role=\"banner\">\n    {{#if  '===' ''}}\n        <h1 class=\"header-logo header-logo--{{theme_settings.logo-position}}\">\n            {{> components/common/store-logo}}\n        </h1>\n    {{else}}\n        <div class=\"header-logo header-logo--{{theme_settings.logo-position}}\">\n            {{> components/common/checkout-store-logo}}\n        </div>\n    {{/if}}\n\n  <div class=\"navPages-container inside-header left-logo\" id=\"menu\" data-menu>\n    <nav class=\"navPages\">\n        <div class=\"navPages-quickSearch right searchbar\">\n            {{> components/common/quick-search}}\n        </div>\n    </nav>\n</div>\n<div class=\"navPages-container inside-header right-logo\" id=\"menu\" data-menu>\n    <nav class=\"navPages\">\n        <div class=\"navPages-quickSearch right searchbar\">\n            {{> components/common/quick-search}}\n        </div>\n    </nav>\n</div>\n<div class=\"navPages-container inside-header center-logo\" id=\"menu\" data-menu>\n    <nav class=\"navPages\">\n        <div class=\"navPages-quickSearch right searchbar\">\n            {{> components/common/quick-search}}\n        </div>\n    </nav>\n</div>\n<div class=\"clear\"></div>\n<div class=\"mobile-menu navPages-container\" id=\"menu\" data-menu>\n  <div class=\"currency-converter\">\n    <ul class=\"navPages-list navPages-list--user\">\n        {{#if currency_selector.currencies.length '>' 1}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action has-subMenu\" href=\"#\" data-collapsible=\"navPages-currency\" aria-controls=\"navPages-currency\" aria-expanded=\"false\">\n                    {{currency_selector.active_currency_code}} <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-chevron-down\"></use></svg></i>\n                </a>\n                <div class=\"navPage-subMenu\" id=\"navPages-currency\" aria-hidden=\"true\" tabindex=\"-1\">\n                    <ul class=\"navPnavPagesage-subMenu-list\">\n                      {{#each currency_selector.currencies}}\n                      <li class=\"dropdown-menu-item\">\n                          <a class=\"currency-name-switcher\" href=\"{{{switch_url}}}\">\n                              {{#if is_active}}\n                              <strong>{{{flag}}} {{name}}</strong>\n                              {{else}}\n                              {{{flag}}} {{name}}\n                              {{/if}}\n                          </a>\n                      </li>\n                      {{/each}}\n                    </ul>\n                </div>\n            </li>\n        {{/if}}\n    </ul>\n  </div>\n        {{> components/common/navigation-menu}}\n</div>\n</header>\n</div>\n</div>\n</div>\n\n<div class=\"main-nav-container{{#if theme_settings.header-layout '!==' 'in-container'}} full-width{{/if}}\">\n  <div class=\"navPages-container main-nav\" id=\"menu\" data-menu>\n        {{> components/common/navigation-menu}}\n  </div>\n</div>\n",
  "components/common/checkout-footer": "<footer class=\"footer\" role=\"contentinfo\">\n\t{{#if settings.show_newsletter_box}}\n    <div class=\"footer-top newsletter\">\n        <div class=\"content newsletter\">\n            <div class=\"\" data-section-type=\"newsletterSubscription\">\n                    {{> components/common/subscription-form}}\n\t\t\t\t\t\t\t\t\t\t<div class=\"clear\"></div>\n            </div>\n      \t\t\t<div class=\"clear\"></div>\n        </div>\n    </div>\n   {{/if}}\n    <div class=\"container\">\n        <section class=\"footer-info\">\n         <article class=\"footer-info-col footer-info-col--small\" data-section-type=\"storeInfo\">\n                <h5 class=\"footer-info-heading\">Contact Us</h5>\n                <div class=\"regular\">\n                    <address>{{nl2br settings.address}}</address>\n                </div>\n        </article>\n\n            <article class=\"footer-info-col footer-info-col--small\" data-section-type=\"footer-webPages\">\n                <h5 class=\"footer-info-heading\">Accounts & Orders</h5>\n                <div class=\"regular\">\n                <ul class=\"footer-info-list\">\n                \t{{#if settings.gift_certificates_enabled}}\n                        <li>\n                            <a href=\"/giftcertificates.php\">Gift Certificates</a>\n                        </li>\n                    {{/if}}\n                        <li>\n                            <a href=\"/wishlist.php\">Wishlist</a>\n                        </li>\n                    {{#if settings.account_creation_enabled}}\n                        {{#if customer}}\n                            <li><a class=\"navUser-action\" href=\"{{urls.account.index}}\">{{lang 'common.account'}}</a></li>\n                            <li><a class=\"navUser-action\" href=\"{{urls.auth.logout}}\">{{lang 'common.logout'}}</a></li>\n                        {{else}}\n                            <li><a class=\"navUser-action\" href=\"{{urls.auth.login}}\">{{lang 'common.login'}}</a>\n                            {{#if settings.account_creation_enabled}}\n                                <span class=\"navUser-or\">{{lang 'common.or'}}</span> <a class=\"navUser-action\" href=\"{{urls.auth.create_account}}\">{{lang 'common.sign_up'}}</a></li>\n                            {{/if}}\n                        {{/if}}\n                    {{/if}}\n\n                    <li>\n                        <a href=\"/account.php?action=order_status\">Order Status</a>\n                    </li>\n                    <li>\n                        <a href=\"/shipping-returns/\">Shipping &amp; Returns</a>\n                    </li>\n                        <a href=\"{{urls.sitemap}}\">{{lang 'sitemap.page_title'}}</a>\n                    </li>\n                </ul>\n                </div>\n            </article>\n\n            <article class=\"footer-info-col footer-info-col--small\" data-section-type=\"footer-categories\">\n                <h5 class=\"footer-info-heading\">Quick Links</h5>\n                <div class=\"regular\">\n                <ul class=\"footer-info-list\">\n                    {{#each pages}}\n                    <li>\n                        <a href=\"{{url}}\">{{name}}</a>\n                    </li>\n                    {{/each}}\n                </ul>\n                </div>\n            </article>\n\n            <article class=\"footer-info-col footer-info-col--small\" data-section-type=\"footer-categories\">\n\n\t\t\t\t\t\t\t{{#if settings.blog_enabled}}\n                <h5 class=\"footer-info-heading recent-posts-heading\">Recent Blog Posts</h5>\n                <div class=\"blog regular\">\n                    <ul class=\"footer-info-list\">\n                        {{#each blog.recent_posts}}\n                            <li><a href=\"{{url}}\">{{title}}</a></li>\n                        {{/each}}\n                    </ul>\n                </div>\n\t\t\t\t\t\t\t{{/if}}\n\n                <div class=\"social-media\">\n                    {{> components/common/social-links}}\n                </div>\n            </article>\n            <div class=\"clear\"></div>\n\t\t\t\t\t\t{{#if theme_settings.geotrust_ssl_common_name}}\n\t\t\t\t\t\t\t<div class=\"footer-geotrust-ssl-seal\">\n\t\t\t\t\t\t\t\t{{> components/common/geotrust-ssl-seal}}\n\t\t\t\t\t\t\t\t<div class=\"clear\"></div>\n\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t{{/if}}\n            {{> components/common/payment-icons}}\n\n        </section>\n\n        <div class=\"clear\"></div>\n    </div>\n\n\n    <div class=\"footer-bottom\">\n        <div class=\"content copyright\">\n\t\t\t\t\t<ul class=\"site-info\">\n\t\t\t\t\t\t\t{{#if theme_settings.show_copyright_footer}}\n\t\t\t\t\t\t\t\t\t\t\t<li class=\"copyright\"><span class=\"current-year\">&copy; <span id=\"copyright_year\"></span> {{settings.store_name}}</span></li>\n\t\t\t\t\t\t\t{{/if}}\n\t\t\t\t\t\t\t{{#if theme_settings.show_sitemap_link}}\n\t\t\t\t\t\t\t\t\t\t\t<li class=\"sitemap\"><a href=\"{{urls.sitemap}}\"> | {{lang 'sitemap.page_title'}}</a></li>\n\t\t\t\t\t\t\t{{/if}}\n\t\t\t\t\t\t\t\t\t\t\t{{#if theme_settings.show_powered_by}}\n\t\t\t\t\t\t\t\t\t\t\t\t<li class=\"built-by\"> | Premium <a target=\"_blank\" href=\"https://www.bigcommerce.com/\">BigCommerce</a> Theme by <a class=\"lone-star-templates\" target=\"_blank\" href=\"http://lonestartemplates.com/\">Lone Star Templates</a></li>\n\t\t\t\t\t\t\t\t\t\t\t{{/if}}\n\t\t\t\t\t\t\t\t\t\t\t<div class=\"clear\"></div>\n\t\t\t\t\t\t</ul>\n        </div>\n    </div>\n    <div id=\"topcontrol\" class=\"backtoTop\" title=\"Scroll Back to Top\">\n          <svg class=\"icon back-to-top-icon\" title=\"submit\" alt=\"submit\"><use xlink:href=\"#icon-caret-square-up\"></use></svg>\n      </div>\n    </footer>\n\n\n<!-- CALL MOST RECENT JQUERY LIBRARY -->\n<script src=\"https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js\" integrity=\"sha512-894YE6QWD5I59HgZOGReFYm4dnWc1Qt5NtvYSaNcOP+u1T9qYdvdihz0PPSiiqn/+/3e7Jo4EaG7TubfWGUrMQ==\" crossorigin=\"anonymous\" referrerpolicy=\"no-referrer\"></script>\n\n<!-- GET CURRENT YEAR FOR COPYRIGHT -->\n<script type=\"text/javascript\">\n    document.getElementById(\"copyright_year\").innerHTML = new Date().getFullYear();\n</script>\n\n<script>\n    $('.custom-pages-nav li a.navPages-action.has-subMenu').hover(\n        function () {\n            $(this).siblings('div#menu .navPage-subMenu').show();\n        },\n        function () {\n            $(this).siblings('div#menu .navPage-subMenu').hide();\n        }\n    );\n</script>\n\n\n<script>\n    $('.mobileMenu-toggle').click(function(e){\n        e.preventDefault();\n        e.stopPropagation();\n        $('body').toggleClass('has-activeNavPages');\n        $('header.header').toggleClass('is-open');\n    });\n\n    if ( $('.countPill').hasClass('countPill--positive') ){\n      $('.top-menu .right').addClass('shift-right');\n    }\n</script>\n",
  "layout/checkout": "<!DOCTYPE html>\n<html>\n<head>\n    <title>{{ head.title }}</title>\n    {{{ head.meta_tags }}}\n    {{{ head.config }}}\n    <link href=\"{{ head.favicon }}\" rel=\"shortcut icon\">\n    {{#block \"head\"}}{{/block}}\n    {{{ stylesheet '/assets/css/theme.css' }}}\n    {{{ checkout.checkout_head }}}\n    {{{ stylesheet '/assets/css/optimized-checkout.css' }}}\n    {{getFontsCollection font-display='block'}}\n</head>\n<body class=\"main{{#if theme_settings.body_width '===' 960}} nine-sixty{{/if}} {{#if theme_settings.body_width '===' 1170}} eleven-seventy {{/if}} {{#if theme_settings.body_width '===' 1280}} twelve-eighty {{/if}} {{#if theme_settings.body_width '===' 1366}} thirteen-sixty-six {{/if}} {{#if theme_settings.body_width '===' 1440}} fourteen-forty {{/if}} {{#if theme_settings.header-layout '!==' 'in-container'}} header-full-width{{/if}} {{#if theme_settings.header-layout '===' 'in-container'}} header-in-container{{/if}}\">\n  <svg data-src=\"{{cdn 'img/icon-sprite.svg'}}\" class=\"icons-svg-sprite\"></svg>\n\n  {{#block \"page\"}}{{/block}} \n\n  \n\n  {{{snippet 'header'}}}\n        <svg data-src=\"{{cdn 'img/icon-sprite.svg'}}\" class=\"icons-svg-sprite\"></svg>\n\n        {{#and settings.privacy_cookie settings.is_eu_ip_address}}\n            {{> components/common/cookie}}\n        {{/and}}\n\n  <script>window.__webpack_public_path__ = \"{{cdn 'assets/dist/'}}\";</script>\n       <script src=\"{{cdn 'assets/dist/theme-bundle.main.js'}}\"></script>\n\n       <script>\n           // Exported in app.js\n           window.stencilBootstrap(\"{{page_type}}\", {{jsContext}}).load();\n       </script>\n\n    {{{footer.scripts}}}\n    {{{snippet 'footer'}}}\n\n</body> \n</html>\n",
  "components/common/checkout-store-logo": "<a href=\"{{urls.home}}\">\n    {{#if settings.store_logo.image}}\n        {{#if theme_settings.logo_size '===' 'original'}}\n            <img class=\"header-logo-image-unknown-size\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n        {{else}}\n            <div class=\"header-logo-image-container\">\n                <img class=\"header-logo-image\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n            </div>\n        {{/if}}\n    {{else}}\n        <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\n    {{/if}}\n</a>\n",
  "components/common/store-logo": "<a href=\"{{urls.home}}\">\n    {{#if settings.store_logo.image}}\n        {{#if theme_settings.logo_size '===' 'original'}}\n            <img class=\"header-logo-image-unknown-size\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n        {{else}}\n            <div class=\"header-logo-image-container\">\n                <img class=\"header-logo-image\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n            </div>\n        {{/if}}\n    {{else}}\n        <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\n    {{/if}}\n</a>\n",
  "components/common/quick-search": "<div class=\"container\">\n    {{{ snippet \"forms_search\"}}}\n    <form class=\"form\" action=\"{{urls.search}}\">\n        <fieldset class=\"form-fieldset\">\n            <div class=\"form-field\">\n                <label class=\"u-hiddenVisually\" for=\"search_query\">{{lang \"search.quick_search.input_label\"}}</label>\n                <input class=\"form-input\" data-search-quick name=\"search_query\" id=\"search_query\" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" autocomplete=\"off\">\n                <div class=\"search-icon\"><svg class=\"header-icon search-icon\" title=\"submit\" alt=\"submit\"><use xlink:href=\"#icon-search\"></use></svg></div>\n                <input class=\"button button--primary form-prefixPostfix-button--postfix\" type=\"submit\" value=\"\">\n            </div>\n        </fieldset>\n        <div class=\"clear\"></div>\n    </form>\n    <div id=\"search-action-buttons\" class=\"search-buttons-container\">\n      <div class=\"align-right\">\n      <!-- <a href=\"#\" class=\"reset quicksearch\" aria-label=\"reset search\" role=\"button\" style=\"inline-block;\">\n        <span class=\"reset\" aria-hidden=\"true\">Reset Search</span>\n      </a> -->\n      <a href=\"#\" class=\"modal-close custom-quick-search\" aria-label=\"{{lang 'common.close'}}\" role=\"button\" style=\"inline-block;\">\n        <span aria-hidden=\"true\">&#215;</span>\n      </a>\n      <div class=\"clear\"></div>\n      </div>\n      <div class=\"clear\"></div>\n    </div>\n      <section id=\"quickSearch\" class=\"quickSearchResults\" data-bind=\"html: results\">\n      </section>\n      <div class=\"clear\"></div>\n</div>\n",
  "components/common/navigation-menu": "<nav class=\"navPages\">\n    <div class=\"navPages-quickSearch right searchbar\">\n        {{> components/common/quick-search}}\n    </div>\n    <ul class=\"navPages-list\">\n        <div class=\"nav-align{{#if theme_settings.mainNav-align '!==' 'left'}} center{{/if}}\">\n        <div class=\"custom-pages-nav\">\n          <div id=\"desktop-menu\">\n            <h2 class=\"container-header mobile\">Main Menu</h2>\n            {{#each pages}}\n                <li class=\"navPages-item\">\n                    {{> components/common/navigation-list}}\n                </li>\n            {{/each}}\n          </div>\n            <div id=\"mobile-menu\">\n              <div class=\"category-menu\">\n                <h2 class=\"container-header mobile\">Shop By Category</h2>\n                {{#each categories}}\n                  <li class=\"navPages-item mobile\">\n                    {{> components/common/navigation-list}}\n                  </li>\n                {{/each}}\n              </div>\n\n            {{#if theme_settings.brands_visibility_mobile_menu '===' true}}\n            <div class=\"brand-menu\">\n              <h2 class=\"container-header mobile\">Shop By Brand</h2>\n              {{#each shop_by_brand}}\n                <li class=\"navPages-item mobile\">\n                  <a class=\"navPages-action\" href=\"{{url}}\">{{name}}</a>\n                </li>\n              {{/each}}\n                  <li class=\"navPages-item mobile\">\n                    <a class=\"navPages-action\" href=\"/brands/\">View all Brands</a>\n                  </li>\n            <div class=\"clear\"></div>\n          </div>\n          {{/if}}\n\n          </div>\n        </div>\n    </ul>\n    <ul class=\"navPages-list navPages-list--user\">\n        {{#if currency_selector.currencies.length '>' 1}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action has-subMenu\" href=\"#\" data-collapsible=\"navPages-currency\" aria-controls=\"navPages-currency\" aria-expanded=\"false\">\n                    {{lang 'common.currency' code=currency_selector.active_currency_code}} <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-chevron-down\"></use></svg></i>\n                </a>\n                <div class=\"navPage-subMenu\" id=\"navPages-currency\" aria-hidden=\"true\" tabindex=\"-1\">\n                    <ul class=\"navPnavPagesage-subMenu-list\">\n                        {{#each currency_selector.currencies}}\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{{switch_url}}}\">\n                                {{#if is_active}}\n                                    <strong>{{name}}</strong>\n                                {{else}}\n                                    {{name}}\n                                {{/if}}\n                            </a>\n                        </li>\n                        {{/each}}\n                    </ul>\n                </div>\n            </li>\n        {{/if}}\n        <li class=\"navPages-item\">\n            <a class=\"navPages-action\" href=\"{{urls.gift_certificate.purchase}}\">{{lang 'common.gift_cert'}}</a>\n        </li>\n        {{#if customer.store_credit.value '>' 0}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action navPages-action--storeCredit\">\n                    {{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}\n                </a>\n            </li>\n        {{/if}}\n        {{#if theme_settings.product_list_display_mode '==' 'list'}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action navPages-action--compare\" href=\"{{urls.compare}}\" data-compare-nav>{{lang 'common.compare'}} <span class=\"countPill countPill--positive countPill--alt\"></span></a>\n            </li>\n        {{/if}}\n        {{#if customer}}\n            <li class=\"navPages-item account\">\n                <a class=\"navPages-action has-subMenu\" href=\"{{urls.account.index}}\" data-collapsible=\"navPages-account\">\n                    {{lang 'common.account'}} <i class=\"fa fa-chevron-down\" aria-hidden=\"true\"></i>\n                </a>\n                <div class=\"navPage-subMenu\" id=\"navPages-account\" aria-hidden=\"true\" tabindex=\"-1\">\n                    <ul class=\"navPage-subMenu-list\">\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.orders.all}}\">{{lang 'account.nav.orders'}}</a>\n                        </li>\n                        {{#if settings.returns_enabled}}\n                            <li class=\"navPage-subMenu-item\">\n                                <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.returns}}\">{{lang 'account.nav.returns'}}</a>\n                            </li>\n                        {{/if}}\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.inbox}}\">{{lang 'account.mobile_nav.messages'}}</a>\n                        </li>\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.addresses}}\">{{lang 'account.nav.addresses'}}</span></a>\n                        </li>\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.wishlists.all}}\">{{lang 'account.mobile_nav.wishlists'}}</a>\n                        </li>\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.recent_items}}\">{{lang 'account.nav.recently_viewed'}}</a>\n                        </li>\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.details}}\">{{lang 'account.nav.settings'}}</a>\n                        </li>\n                    </ul>\n                </div>\n            </li>\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action\" href=\"{{urls.auth.logout}}\">{{lang 'common.logout'}}</a>\n            </li>\n        {{else}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action\" href=\"{{urls.auth.login}}\">{{lang 'common.login'}}</a> {{lang 'common.or'}} <a class=\"navPages-action\" href=\"{{urls.auth.create_account}}\">{{lang 'common.sign_up'}}</a>\n            </li>\n        {{/if}}\n    </ul>\n</nav>\n",
  "components/common/geotrust-ssl-seal": "<table width=\"135\" border=\"0\" cellpadding=\"2\" cellspacing=\"0\" title=\"Click to Verify - This site chose GeoTrust SSL for secure e-commerce and confidential communications.\">\n    <tr>\n        <td width=\"135\" align=\"center\" valign=\"top\">\n            <script type=\"text/javascript\" src=\"https://seal.geotrust.com/getgeotrustsslseal?host_name={{theme_settings.geotrust_ssl_common_name}}&amp;size={{theme_settings.geotrust_ssl_seal_size}}&amp;lang={{locale_name}}\"></script><br />\n            <a href=\"http://www.geotrust.com/ssl/\" target=\"_blank\"  style=\"color:#000000; text-decoration:none; font:bold 7px verdana,sans-serif; letter-spacing:.5px; text-align:center; margin:0px; padding:0px;\"></a>\n        </td>\n    </tr>\n</table>\n",
  "components/common/social-links": "{{#if social_media}}\n    <h5 class=\"footer-info-heading\">Connect with Us:</h5>\n    <ul class=\"socialLinks socialLinks--alt\">\n        {{#each social_media}}\n            <li class=\"socialLinks-item\">\n                {{#if display_name '===' 'google+'}}\n                    <a class=\"icon icon--google\" href=\"{{url}}\" target=\"_blank\">\n                        <svg><use xlink:href=\"#icon-google\" /></svg>\n                    </a>\n                {{else}}\n                    <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\">\n                        <svg><use xlink:href=\"#icon-{{display_name}}\" /></svg>\n                    </a>\n                {{/if}}\n            </li>\n        {{/each}}\n    </ul>\n{{/if}}\n",
  "components/common/cookie": "<div class=\"cookieMessage\">\n    <div class=\"cookieMessage-container\">\n        <div class=\"cookieMessage-text\">\n            <p>{{settings.privacy_cookie}}</p>\n        </div>\n        <button class=\"button button--primary button--small\" data-privacy-accept>{{lang 'cookies.accept'}}</button>\n    </div>\n</div>\n",
  "components/common/payment-icons": "<div class=\"footer-payment-icons\">\n    {{#if theme_settings.show_accept_amazon_pay}}\n        <svg class=\"footer-payment-icon amazonpay\"><use xlink:href=\"#icon-logo-amazonpay\"></use></svg>\n    {{/if}}\n    {{#if theme_settings.show_accept_amex}}\n        <svg class=\"footer-payment-icon amex\"><use xlink:href=\"#icon-logo-american-express\"></use></svg>\n    {{/if}}\n    {{#if theme_settings.show_accept_apple_pay}}\n        <svg class=\"footer-payment-icon applepay\"><use xlink:href=\"#icon-logo-applepay\"></use></svg>\n    {{/if}}\n    {{#if theme_settings.show_accept_chase_pay}}\n        <svg class=\"footer-payment-icon chasepay\"><use xlink:href=\"#icon-logo-chasepay\"></use></svg>\n    {{/if}}\n    {{#if theme_settings.show_accept_discover}}\n        <svg class=\"footer-payment-icon discover\"><use xlink:href=\"#icon-logo-discover\"></use></svg>\n    {{/if}}\n    {{#if theme_settings.show_accept_mastercard}}\n        <svg class=\"footer-payment-icon mastercard\"><use xlink:href=\"#icon-logo-mastercard\"></use></svg>\n    {{/if}}\n    {{#if theme_settings.show_accept_masterpass}}\n        <svg class=\"footer-payment-icon masterpass\"><use xlink:href=\"#icon-logo-masterpass\"></use></svg>\n    {{/if}}\n    {{#if theme_settings.show_accept_paypal}}\n        <svg class=\"footer-payment-icon paypal\"><use xlink:href=\"#icon-logo-paypal\"></use></svg>\n    {{/if}}\n    {{#if theme_settings.show_accept_visa}}\n        <svg class=\"footer-payment-icon visa\"><use xlink:href=\"#icon-logo-visa\"></use></svg>\n    {{/if}}\n    {{#if theme_settings.show_accept_visa_pay}}\n        <svg class=\"footer-payment-icon visapay\"><use xlink:href=\"#icon-logo-visapay\"></use></svg>\n    {{/if}}\n    <div class=\"clear\"></div>\n</div>\n\n<div class=\"global-region container-width right-of-payment-icons\"> <!-- can be used for SSL Certificate Seal -->\n  {{{region name=\"footer_right_of_payment_icons--global\"}}}\n</div>\n",
  "components/common/subscription-form": "<h5 class=\"footer-info-heading\">{{lang 'newsletter.subscribe'}}&nbsp;<span class=\"smaller lighter lowercase\">{{lang 'newsletter.subscribe_intro'}}</span></h5>\n<form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\n    <fieldset class=\"form-fieldset\">\n        <input type=\"hidden\" name=\"action\" value=\"subscribe\">\n        <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\n        <input type=\"hidden\" name=\"check\" value=\"1\">\n        <div class=\"form-field\">\n            <label class=\"form-label u-hiddenVisually\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\n            <div class=\"form-prefixPostfix wrap\">\n                <input class=\"form-input\" id=\"nl_email\" name=\"nl_email\" type=\"email\" value=\"{{customer.email}}\" placeholder=\"Email\">\n                <input class=\"button button--primary form-prefixPostfix-button--postfix\" type=\"submit\" value=\"Join\">\n                <div class=\"clear\"></div>\n            </div>\n            <div class=\"clear\"></div>\n        </div>\n        <div class=\"clear\"></div>\n    </fieldset>\n</form>\n<div class=\"clear\"></div>\n",
  "components/common/navigation-list": "{{#if children}}\n<a class=\"navPages-action has-subMenu\" href=\"{{url}}\">\n    {{name}} <i class=\"fa fa-chevron-down main-nav\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\n</a>\n<div class=\"navPage-subMenu\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n    <ul class=\"navPage-subMenu-list\">\n        {{#each children}}\n            <li class=\"navPage-subMenu-item\">\n                {{#if children}}\n                    <a\n                        class=\"navPage-subMenu-action navPages-action has-subMenu\"\n                        href=\"{{url}}\">\n                        {{name}} <i class=\"fa fa-chevron-down\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\n                    </a>\n                    <ul class=\"navPage-childList\" id=\"navPages-{{id}}\">\n                        {{#each children}}\n                        <li class=\"navPage-childList-item\">\n                            <a class=\"navPage-childList-action navPages-action\" href=\"{{url}}\">{{name}}</a>\n                        </li>\n                        {{/each}}\n                    </ul>\n                {{else}}\n                    <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\">{{name}}</a>\n                {{/if}}\n            </li>\n        {{/each}}\n    </ul>\n</div>\n{{else}}\n<a class=\"navPages-action\" href=\"{{url}}\">{{name}}</a>\n{{/if}}\n"
}