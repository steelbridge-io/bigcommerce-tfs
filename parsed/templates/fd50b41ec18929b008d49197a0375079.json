{
  "components/common/breadcrumbs": "<ul class=\"breadcrumbs\">\n    {{#each breadcrumbs}}\n        <li class=\"breadcrumb {{#if @last}}is-active{{/if}}\">\n            {{#if url}}\n                <a href=\"{{url}}\" class=\"breadcrumb-label\">{{name}}</a>\n            {{else}}\n                <span class=\"breadcrumb-label\">{{name}}</span>\n            {{/if}}\n        </li>\n    {{/each}}\n</ul>\n"
}