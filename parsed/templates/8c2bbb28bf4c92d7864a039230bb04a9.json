{
  "components/common/subscription-form": "<h5 class=\"footer-info-heading\">{{lang 'newsletter.subscribe'}}&nbsp;<span class=\"smaller lighter lowercase\">{{lang 'newsletter.subscribe_intro'}}</span></h5>\n<form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\n    <fieldset class=\"form-fieldset\">\n        <input type=\"hidden\" name=\"action\" value=\"subscribe\">\n        <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\n        <input type=\"hidden\" name=\"check\" value=\"1\">\n        <div class=\"form-field\">\n            <label class=\"form-label u-hiddenVisually\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\n            <div class=\"form-prefixPostfix wrap\">\n                <input class=\"form-input\" id=\"nl_email\" name=\"nl_email\" type=\"email\" value=\"{{customer.email}}\" placeholder=\"Email\">\n                <input class=\"button button--primary form-prefixPostfix-button--postfix\" type=\"submit\" value=\"Join\">\n                <div class=\"clear\"></div>\n            </div>\n            <div class=\"clear\"></div>\n        </div>\n        <div class=\"clear\"></div>\n    </fieldset>\n</form>\n<div class=\"clear\"></div>\n"
}