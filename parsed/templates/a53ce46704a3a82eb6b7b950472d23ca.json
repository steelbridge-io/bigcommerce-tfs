{
  "components/carousel": "<section class=\"heroCarousel\"\n    data-slick='{\n        \"arrows\": {{arrows}},\n        \"mobileFirst\": true,\n        \"slidesToShow\": 1,\n        \"slidesToScroll\": 1,\n        \"autoplay\": true,\n        \"autoplaySpeed\": {{carousel.swap_frequency}},\n        \"lazyLoad\": \"anticipated\"\n    }'>\n    {{#each carousel.slides}}\n    <a href=\"{{url}}\">\n        <div class=\"heroCarousel-slide {{#if ../theme_settings.homepage_stretch_carousel_images}}stretch{{/if}} {{#if @first}}heroCarousel-slide--first{{/if}}\">\n            <div class=\"heroCarousel-image-wrapper\">\n              <img class=\"heroCarousel-image\" src=\"{{image}}\" alt=\"{{alt_text}}\" title=\"{{alt_text}}\"/>\n            </div>\n            {{#if heading}}\n                {{> components/carousel-content show_background=true}}\n            {{else}}\n                {{#if text}}\n                    {{> components/carousel-content show_background=true}}\n                {{else}}\n                    {{#if button_text}}\n                        {{> components/carousel-content show_background=false}}\n                    {{/if}}\n                {{/if}}\n            {{/if}}\n        </div>\n    </a>\n    {{/each}}\n</section>\n",
  "components/carousel-content": "<div class=\"heroCarousel-content{{#unless show_background}} heroCarousel-content--empty{{/unless}}\">\n    <h1 class=\"heroCarousel-title\">{{{heading}}}</h1>\n    <p class=\"heroCarousel-description\">{{{text}}}</p>\n    {{#if button_text}}\n        <span class=\"heroCarousel-action button button--primary button--large\">{{button_text}}</span>\n    {{/if}}\n</div>\n"
}