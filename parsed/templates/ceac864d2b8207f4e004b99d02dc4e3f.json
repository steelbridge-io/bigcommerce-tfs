{
  "components/blog/post-page": "<article class=\"blog\">\n    <div class=\"blog-post-figure\">\n        {{#if post.thumbnail}}\n            <figure class=\"blog-thumbnail\">\n                <a href=\"{{post.url}}\">\n                    <img class=\"lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"{{getImage post.thumbnail 'blog_size'}}\" alt=\"{{post.thumbnail.alt}}\" title=\"{{post.thumbnail.alt}}\">\n                </a>\n            </figure>\n        {{/if}}\n    </div>\n\n    <div class=\"blog-post-body\">\n        <header class=\"blog-header\">\n            <h2 class=\"blog-title\">\n                <a href=\"{{post.url}}\" data-maxlength=\"24\">{{post.title}}</a>\n            </h2>\n            <p class=\"blog-date\" data-maxlength=\"55\">{{#if post.author}}{{lang 'blog.posted_by' name=post.author}} on {{/if}}{{post.date_published}}</p>\n        </header>\n\n        <div class=\"blog-post\">\n            {{#if post.body}}\n                {{{post.body}}}\n            {{else}}\n                {{{post.summary}}}\n            {{/if}}\n        </div>\n\n        {{#if post.show_read_more}}\n        <div class=\"read-more\">\n          <a class=\"read-more-button\" href=\"{{url}}\">read more</a>\n        </div>\n        {{/if}}\n\n        <div class=\"share\">\n          {{>components/common/share}}\n        </div>\n\n    </div>\n</article>\n",
  "components/common/share": "{{#if settings.add_this.buttons}}\n    {{assignVar 'encodedUrl' (encodeURI url)}}\n    {{assignVar 'encodedTitle' (encodeURI head.title)}}\n    <div>\n        <ul class=\"socialLinks\">\n            {{#each settings.add_this.buttons}}\n                <li class=\"socialLinks-item socialLinks-item--{{service}}\">\n                    {{#if service '===' 'facebook'}}\n                    <a class=\"socialLinks__link icon icon--{{service}}\"\n                       title=\"{{{capitalize service}}}\"\n                       href=\"https://facebook.com/sharer/sharer.php?u={{getVar 'encodedUrl'}}\"\n                       target=\"_blank\"\n                       rel=\"noopener\"\n                       {{{annotation}}}\n                    >\n                        <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\n                        <svg>\n                            <use xlink:href=\"#icon-facebook\"/>\n                        </svg>\n                    </a>\n                    {{else if service '===' 'email'}}\n                    <a class=\"socialLinks__link icon icon--{{service}}\"\n                       title=\"{{{capitalize service}}}\"\n                       href=\"mailto:?subject={{getVar 'encodedTitle'}}&amp;body={{getVar 'encodedUrl'}}\"\n                       target=\"_self\"\n                       rel=\"noopener\"\n                       {{{annotation}}}\n                    >\n                        <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\n                        <svg>\n                            <use xlink:href=\"#icon-envelope\"/>\n                        </svg>\n                    </a>\n                    {{else if service '===' 'print'}}\n                    <a class=\"socialLinks__link icon icon--{{service}}\"\n                       title=\"{{{capitalize service}}}\"\n                       onclick=\"window.print();return false;\"\n                       {{{annotation}}}\n                    >\n                        <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\n                        <svg>\n                            <use xlink:href=\"#icon-print\"/>\n                        </svg>\n                    </a>\n                    {{else if service '===' 'twitter'}}\n                    <a class=\"socialLinks__link icon icon--{{service}}\"\n                       href=\"https://twitter.com/intent/tweet/?text={{getVar 'encodedTitle'}}&amp;url={{getVar 'encodedUrl'}}\"\n                       target=\"_blank\"\n                       rel=\"noopener\"\n                       title=\"{{{capitalize service}}}\"\n                       {{{annotation}}}\n                    >\n                        <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\n                        <svg>\n                            <use xlink:href=\"#icon-twitter\"/>\n                        </svg>\n                    </a>\n                    {{else if service '===' 'linkedin'}}\n                    <a class=\"socialLinks__link icon icon--{{service}}\"\n                       title=\"{{{capitalize service}}}\"\n                       href=\"https://www.linkedin.com/shareArticle?mini=true&amp;url={{getVar 'encodedUrl'}}&amp;title={{getVar 'encodedTitle'}}&amp;summary={{getVar 'encodedTitle'}}&amp;source={{getVar 'encodedUrl'}}\"\n                       target=\"_blank\"\n                       rel=\"noopener\"\n                       {{{annotation}}}\n                    >\n                        <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\n                        <svg>\n                            <use xlink:href=\"#icon-linkedin\"/>\n                        </svg>\n                    </a>\n                    {{else if service '===' 'pinterest'}}\n                    <a class=\"socialLinks__link icon icon--{{service}}\"\n                       title=\"{{{capitalize service}}}\"\n                       href=\"https://pinterest.com/pin/create/button/?url={{getVar 'encodedUrl'}}&amp;description={{getVar 'encodedTitle'}}\"\n                       target=\"_blank\"\n                       rel=\"noopener\"\n                       {{{annotation}}}\n                    >\n                        <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\n                        <svg>\n                            <use xlink:href=\"#icon-pinterest\"/>\n                        </svg>\n                    </a>\n                    {{/if}}\n                </li>\n            {{/each}}\n        </ul>\n    </div>\n{{/if}}\n{{#if settings.facebook_like_button.enabled}}\n    <iframe\n        class=\"facebook{{#if settings.facebook_like_button.verb '===' 'recommend'}}Recommend{{else}}Like{{/if}}Btn\"\n        style=\"border:none; overflow:hidden; margin: 0; padding: 0; position:absolute\"\n        src=\"https://www.facebook.com/plugins/like.php?href={{settings.facebook_like_button.href}}&amp;layout=button_count{{#if settings.facebook_like_button.verb '===' 'recommend'}}&amp;action=recommend{{/if}}&amp;colorscheme=light&amp;height=20\"\n        scrolling=\"no\"\n        frameborder=\"0\"\n        allowTransparency=\"true\">\n    </iframe>\n{{/if}}\n"
}