{
  "components/common/header": "<div class=\"header-container {{#if theme_settings.sticky_topMenu '===' true}}sticky-header{{/if}}{{#if settings.gift_certificates_enabled}} gift-certificates-enabled {{/if}}\">\n<div class=\"top-menu\">\n    <div class=\"content\">\n\n      <!-- NEW BUTTON TO TRIGGER MAIN NAV WHEN STICKY -->\n      <!-- <div class=\"alt-toggle\">\n        <h2 class=\"container-header btn\"><span class=\"down-arrrow burger-menu\"><i class=\"burger open\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-Menu\" /></svg></i><i class=\"menu close\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-close\" /></svg></i></span></h2>\n        <h2 class=\"container-header text\"><span class=\"mega-shop-text\"></span></h2>\n      </div> -->\n\n         <a href=\"#\" class=\"mobileMenu-toggle\" data-mobile-menu-toggle=\"menu\">\n            <span class=\"mobileMenu-toggleIcon\">Toggle menu</span>\n        </a>\n\n        <!-- OPTIONAL SHORT MESSAGE LINK -->\n        <!-- <div class=\"left\">\n          <a class=\"optional-message-link\" href=\"\">Put an optional message here.</a>\n        </div> -->\n\n        <div class=\"left phone-number\">\n            {{#if settings.phone_number}}\n                {{settings.phone_number}}\n            {{/if}}\n        </div>\n\n        {{>components/common/currency-selector}}\n\n        {{#if theme_settings.sticky_topMenu '===' true}}\n\n        {{/if}}\n\n        <div class=\"right account-links\">\n            <ul>\n               {{#if settings.gift_certificates_enabled}}\n                    <li class=\"navUser-item gift-certficates\">\n                        <a class=\"navUser-action\" href=\"{{urls.gift_certificate.purchase}}\">{{lang 'cart.gift_certificate'}}</a>\n                    </li>\n                {{/if}}\n                {{#if settings.account_creation_enabled}}\n                    <li class=\"navUser-item navUser-item--account\">\n                        {{#if customer}}\n                            <a class=\"navUser-action account\" href=\"{{urls.account.index}}\"><i class=\"fa fa-user\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-user-alt\" /></svg></i></a>\n                            <a class=\"navUser-action logout\" href=\"{{urls.auth.logout}}\">{{lang 'common.logout'}}</a>\n                        {{else}}\n                            <a class=\"navUser-action\" href=\"{{urls.auth.login}}\"><!-- <i class=\"fa fa-user\" aria-hidden=\"true\"></i> --><a class=\"navUser-action login\" href=\"{{urls.auth.login}}\">{{lang 'common.login'}}</a><span class=\"or-text\">or</span><a class=\"navUser-action create\" href=\"{{urls.auth.create_account}}\">{{lang 'Sign Up'}}</a>\n                        {{/if}}\n                     </li>\n                {{/if}}\n        <li class=\"navUser-item navUser-item--cart {{#if cart.items}} countPill--positive{{/if}}\">\n            <a\n                class=\"navUser-action\"\n                data-cart-preview\n                data-dropdown=\"cart-preview-dropdown\"\n                data-options=\"align:right\"\n                href=\"{{urls.cart}}\">\n                <span class=\"navUser-item-cartLabel\"><i class=\"cart-icon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-cart\" /></svg></i></span> <span class=\"countPill{{#if cart.items}} countPill--positive{{/if}} cart-quantity\">{{cart.quantity}}</span>\n            </a>\n\n            <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\n        </li>\n            </ul>\n        </div>\n        <div class=\"clear\"></div>\n    </div>\n</div>\n\n<div class=\"header-container full-width {{#if theme_settings.slider_width '===' 'full-width'}} full-width-auto {{/if}} full-width{{#if theme_settings.header-layout '!==' 'full-width'}} in-container{{/if}}\">\n  <div class=\"header-padding{{#if theme_settings.header-layout '===' 'in-container-with-padding-no-margin'}} in-container-with-padding-no-margin{{/if}}\">\n  <div class=\"header-padding{{#if theme_settings.header-layout '===' 'in-container-with-padding'}} in-container-with-padding{{/if}}\">\n  <header class=\"header\" role=\"banner\">\n    {{#if page_type '===' 'default'}}\n        <div class=\"header-logo header-logo--{{theme_settings.logo-position}}\">\n            {{> components/common/store-logo}}\n        </div>\n    {{else}}\n        <div class=\"header-logo header-logo--{{theme_settings.logo-position}}\">\n            {{> components/common/store-logo}}\n        </div>\n    {{/if}}\n\n{{#if theme_settings.logo-position '===' 'left'}}\n  <div class=\"navPages-container inside-header left-logo search-container not-sticky\" id=\"menu\" data-menu>\n    <nav class=\"navPages\">\n        <div class=\"navPages-quickSearch right searchbar\">\n            {{> components/common/quick-search}}\n        </div>\n    </nav>\n</div>\n{{/if}}\n{{#if theme_settings.logo-position '===' 'right'}}\n<div class=\"navPages-container inside-header right-logo search-container not-sticky\" id=\"menu\" data-menu>\n    <nav class=\"navPages\">\n        <div class=\"navPages-quickSearch right searchbar\">\n            {{> components/common/quick-search}}\n        </div>\n    </nav>\n</div>\n{{/if}}\n{{#if theme_settings.logo-position '===' 'center'}}\n<div class=\"navPages-container inside-header center-logo search-container not-sticky\" id=\"menu\" data-menu>\n    <nav class=\"navPages\">\n        <div class=\"navPages-quickSearch right searchbar\">\n            {{> components/common/quick-search}}\n        </div>\n    </nav>\n</div>\n{{/if}}\n<div class=\"clear\"></div>\n<div class=\"mobile-menu navPages-container\" id=\"menu\" data-menu>\n  <div class=\"currency-converter\">\n    <ul class=\"navPages-list navPages-list--user\">\n        {{#if currency_selector.currencies.length '>' 1}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action has-subMenu\" href=\"#\" data-collapsible=\"navPages-currency\" aria-controls=\"navPages-currency\" aria-expanded=\"false\">\n                    {{currency_selector.active_currency_code}} <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-chevron-down\"></use></svg></i>\n                </a>\n                <div class=\"navPage-subMenu\" id=\"navPages-currency\" aria-hidden=\"true\" tabindex=\"-1\">\n                    <ul class=\"navPnavPagesage-subMenu-list\">\n                      {{#each currency_selector.currencies}}\n                      <li class=\"dropdown-menu-item\">\n                          <a class=\"currency-name-switcher\" href=\"{{{switch_url}}}\">\n                              {{#if is_active}}\n                              <strong>{{{flag}}} {{name}}</strong>\n                              {{else}}\n                              {{{flag}}} {{name}}\n                              {{/if}}\n                          </a>\n                      </li>\n                      {{/each}}\n                    </ul>\n                </div>\n            </li>\n        {{/if}}\n    </ul>\n  </div>\n        {{> components/common/navigation-menu}}\n</div>\n</header>\n</div>\n</div>\n</div>\n\n<div class=\"main-nav-container{{#if theme_settings.header-layout '!==' 'in-container'}} full-width{{/if}} {{#if theme_settings.header-layout '===' 'in-container'}} in-container{{/if}}\">\n  <div class=\"navPages-container main-nav\" id=\"menu\" data-menu>\n        {{> components/common/navigation-menu}}\n  </div>\n</div>\n</div>\n{{{region name=\"header_bottom--global\"}}}\n</div>\n",
  "components/common/currency-selector": "{{#if currency_selector.currencies.length '>' 1}}\n<div class=\"currency-converter\">\n<li class=\"navUser-item currency-menu\">\n    <a class=\"navUser-action has-dropdown\" title=\"Currency Selector\" alt=\"Currency Selector\" href=\"#\" data-dropdown=\"currencySelection\" aria-controls=\"currencySelection\" aria-expanded=\"false\">{{currency_selector.active_currency_code}} <i class=\"icon currency\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i></a>\n    <ul class=\"dropdown-menu\" id=\"currencySelection\" data-dropdown-content aria-hidden=\"true\" tabindex=\"-1\">\n        {{#each currency_selector.currencies}}\n        <li class=\"dropdown-menu-item\">\n            <a class=\"currency-name-switcher\" href=\"{{{switch_url}}}\">\n                {{#if is_active}}\n                <strong>{{{flag}}} {{name}}</strong>\n                {{else}}\n                {{{flag}}} {{name}}\n                {{/if}}\n            </a>\n        </li>\n        {{/each}}\n    </ul>\n</li>\n</div>\n{{/if}}\n",
  "components/common/store-logo": "<a href=\"{{urls.home}}\">\n    {{#if settings.store_logo.image}}\n        {{#if theme_settings.logo_size '===' 'original'}}\n            <img class=\"header-logo-image-unknown-size\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n        {{else}}\n            <div class=\"header-logo-image-container\">\n                <img class=\"header-logo-image\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n            </div>\n        {{/if}}\n    {{else}}\n        <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\n    {{/if}}\n</a>\n",
  "components/common/quick-search": "<div class=\"container\">\n    {{{ snippet \"forms_search\"}}}\n    <form class=\"form\" action=\"{{urls.search}}\">\n        <fieldset class=\"form-fieldset\">\n            <div class=\"form-field\">\n                <label class=\"u-hiddenVisually\" for=\"search_query\">{{lang \"search.quick_search.input_label\"}}</label>\n                <input class=\"form-input\" data-search-quick name=\"search_query\" id=\"search_query\" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" autocomplete=\"off\">\n                <div class=\"search-icon\"><svg class=\"header-icon search-icon\" title=\"submit\" alt=\"submit\"><use xlink:href=\"#icon-search\"></use></svg></div>\n                <input class=\"button button--primary form-prefixPostfix-button--postfix\" type=\"submit\" value=\"\">\n            </div>\n        </fieldset>\n        <div class=\"clear\"></div>\n    </form>\n    <div id=\"search-action-buttons\" class=\"search-buttons-container\">\n      <div class=\"align-right\">\n      <!-- <a href=\"#\" class=\"reset quicksearch\" aria-label=\"reset search\" role=\"button\" style=\"inline-block;\">\n        <span class=\"reset\" aria-hidden=\"true\">Reset Search</span>\n      </a> -->\n      <a href=\"#\" class=\"modal-close custom-quick-search\" aria-label=\"{{lang 'common.close'}}\" role=\"button\" style=\"inline-block;\">\n        <span aria-hidden=\"true\">&#215;</span>\n      </a>\n      <div class=\"clear\"></div>\n      </div>\n      <div class=\"clear\"></div>\n    </div>\n      <section id=\"quickSearch\" class=\"quickSearchResults\" data-bind=\"html: results\">\n      </section>\n      <div class=\"clear\"></div>\n</div>\n",
  "components/common/navigation-menu": "<nav class=\"navPages\">\n    <div class=\"navPages-quickSearch right searchbar\">\n        {{> components/common/quick-search}}\n    </div>\n    <ul class=\"navPages-list\">\n        <div class=\"nav-align{{#if theme_settings.mainNav-align '!==' 'left'}} center{{/if}}\">\n        <div class=\"custom-pages-nav\">\n          <div id=\"desktop-menu\">\n            <h2 class=\"container-header mobile\">Main Menu</h2>\n            {{#each pages}}\n                <li class=\"navPages-item\">\n                    {{> components/common/navigation-list}}\n                </li>\n            {{/each}}\n          </div>\n            <div id=\"mobile-menu\">\n              <div class=\"category-menu\">\n                <h2 class=\"container-header mobile\">Shop By Category</h2>\n                {{#each categories}}\n                  <li class=\"navPages-item mobile\">\n                    {{> components/common/navigation-list}}\n                  </li>\n                {{/each}}\n              </div>\n\n            {{#if theme_settings.brands_visibility_mobile_menu '===' true}}\n            <div class=\"brand-menu\">\n              <h2 class=\"container-header mobile\">Shop By Brand</h2>\n              {{#each shop_by_brand}}\n                <li class=\"navPages-item mobile\">\n                  <a class=\"navPages-action\" href=\"{{url}}\">{{name}}</a>\n                </li>\n              {{/each}}\n                  <li class=\"navPages-item mobile\">\n                    <a class=\"navPages-action\" href=\"/brands/\">View all Brands</a>\n                  </li>\n            <div class=\"clear\"></div>\n          </div>\n          {{/if}}\n\n          </div>\n        </div>\n    </ul>\n    <ul class=\"navPages-list navPages-list--user\">\n        {{#if currency_selector.currencies.length '>' 1}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action has-subMenu\" href=\"#\" data-collapsible=\"navPages-currency\" aria-controls=\"navPages-currency\" aria-expanded=\"false\">\n                    {{lang 'common.currency' code=currency_selector.active_currency_code}} <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-chevron-down\"></use></svg></i>\n                </a>\n                <div class=\"navPage-subMenu\" id=\"navPages-currency\" aria-hidden=\"true\" tabindex=\"-1\">\n                    <ul class=\"navPnavPagesage-subMenu-list\">\n                        {{#each currency_selector.currencies}}\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{{switch_url}}}\">\n                                {{#if is_active}}\n                                    <strong>{{name}}</strong>\n                                {{else}}\n                                    {{name}}\n                                {{/if}}\n                            </a>\n                        </li>\n                        {{/each}}\n                    </ul>\n                </div>\n            </li>\n        {{/if}}\n        <li class=\"navPages-item\">\n            <a class=\"navPages-action\" href=\"{{urls.gift_certificate.purchase}}\">{{lang 'common.gift_cert'}}</a>\n        </li>\n        {{#if customer.store_credit.value '>' 0}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action navPages-action--storeCredit\">\n                    {{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}\n                </a>\n            </li>\n        {{/if}}\n        {{#if theme_settings.product_list_display_mode '==' 'list'}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action navPages-action--compare\" href=\"{{urls.compare}}\" data-compare-nav>{{lang 'common.compare'}} <span class=\"countPill countPill--positive countPill--alt\"></span></a>\n            </li>\n        {{/if}}\n        {{#if customer}}\n            <li class=\"navPages-item account\">\n                <a class=\"navPages-action has-subMenu\" href=\"{{urls.account.index}}\" data-collapsible=\"navPages-account\">\n                    {{lang 'common.account'}} <i class=\"fa fa-chevron-down\" aria-hidden=\"true\"></i>\n                </a>\n                <div class=\"navPage-subMenu\" id=\"navPages-account\" aria-hidden=\"true\" tabindex=\"-1\">\n                    <ul class=\"navPage-subMenu-list\">\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.orders.all}}\">{{lang 'account.nav.orders'}}</a>\n                        </li>\n                        {{#if settings.returns_enabled}}\n                            <li class=\"navPage-subMenu-item\">\n                                <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.returns}}\">{{lang 'account.nav.returns'}}</a>\n                            </li>\n                        {{/if}}\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.inbox}}\">{{lang 'account.mobile_nav.messages'}}</a>\n                        </li>\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.addresses}}\">{{lang 'account.nav.addresses'}}</span></a>\n                        </li>\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.wishlists.all}}\">{{lang 'account.mobile_nav.wishlists'}}</a>\n                        </li>\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.recent_items}}\">{{lang 'account.nav.recently_viewed'}}</a>\n                        </li>\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.details}}\">{{lang 'account.nav.settings'}}</a>\n                        </li>\n                    </ul>\n                </div>\n            </li>\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action\" href=\"{{urls.auth.logout}}\">{{lang 'common.logout'}}</a>\n            </li>\n        {{else}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action\" href=\"{{urls.auth.login}}\">{{lang 'common.login'}}</a> {{lang 'common.or'}} <a class=\"navPages-action\" href=\"{{urls.auth.create_account}}\">{{lang 'common.sign_up'}}</a>\n            </li>\n        {{/if}}\n    </ul>\n</nav>\n",
  "components/common/navigation-list": "{{#if children}}\n<a class=\"navPages-action has-subMenu\" href=\"{{url}}\">\n    {{name}} <i class=\"fa fa-chevron-down main-nav\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\n</a>\n<div class=\"navPage-subMenu\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n    <ul class=\"navPage-subMenu-list\">\n        {{#each children}}\n            <li class=\"navPage-subMenu-item\">\n                {{#if children}}\n                    <a\n                        class=\"navPage-subMenu-action navPages-action has-subMenu\"\n                        href=\"{{url}}\">\n                        {{name}} <i class=\"fa fa-chevron-down\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\n                    </a>\n                    <ul class=\"navPage-childList\" id=\"navPages-{{id}}\">\n                        {{#each children}}\n                        <li class=\"navPage-childList-item\">\n                            <a class=\"navPage-childList-action navPages-action\" href=\"{{url}}\">{{name}}</a>\n                        </li>\n                        {{/each}}\n                    </ul>\n                {{else}}\n                    <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\">{{name}}</a>\n                {{/if}}\n            </li>\n        {{/each}}\n    </ul>\n</div>\n{{else}}\n<a class=\"navPages-action\" href=\"{{url}}\">{{name}}</a>\n{{/if}}\n"
}